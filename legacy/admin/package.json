{
  "name": "@smoex-learn-jp/cms",
  "version": "0.1.0",
  "homepage": "https://admin.smoex.com",
  "proxy": "https://admin.smoex.com",
  "private": true,
  "scripts": {
    "start": "craco start",
    "test": "craco test",
    "eject": "craco eject",
    "build": "cross-env NODE_ENV=production craco build",
    "build:prod": "cross-env NODE_ENV=production craco build && npm run upload:prod",
    "upload:prod": "smoex-scripts upload ./build",
    "package": "npm run build && npm run upload:prod",
    "format": "npm run format:tslint && npm run format:prettier",
    "format:tslint": " tslint --fix --config ./tslint.json 'src/**/*.{ts,tsx}'",
    "format:stylelint": "stylelint --fix  --syntax scss 'src/**/*.*css'",
    "format:prettier": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "analyze": "cross-env npm_config_report=true npm run build"
  },
  "dependencies": {
    "@smoex-business/user": "^0.0.4",
    "@smoex-learn-jp/business": "^0.0.1",
    "@smoex-web/basic": "^0.0.5",
    "basic-kit-browser": "^0.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dom-basic-kit": "^0.1.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux-async-kit": "^0.0.16",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-scripts": "^3.1.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.11.1",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "typescript": "3.6.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
